@model AddressCreateDto


<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
<meta name="description" content="" />
<meta name="author" content="" />
<title>One Page Wonder - Start Bootstrap Template</title>
<link rel="icon" type="image/x-icon" href="assets/favicon.ico" />
<!-- Font Awesome icons (free version)-->
<script src="https://use.fontawesome.com/releases/v6.1.0/js/all.js" crossorigin="anonymous"></script>
<!-- Google fonts-->
<link href="https://fonts.googleapis.com/css?family=Catamaran:100,200,300,400,500,600,700,800,900" rel="stylesheet" />
<link href="https://fonts.googleapis.com/css?family=Lato:100,100i,300,300i,400,400i,700,700i,900,900i" rel="stylesheet" />
<!-- Core theme CSS (includes Bootstrap)-->
<link href="~/css/create/css/styles.css" rel="stylesheet" />


<!-- Bootstrap core JS-->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
<!-- Core theme JS-->
<script src="~/css/create/js/scripts.js"></script>



<div class="container-fluid">
    <form asp-action="Address" asp-controller="Order" method="post" enctype="multipart/form-data">
        <div class="mb-3">
            <label>Country</label>
            <input type="text" class="form-control" asp-for="@Model.Country" placeholder="Country" />
        </div>
        <div class="mb-3">
            <label class="form-check-label">City</label>
            <input type="text" asp-for="@Model.City" class="form-control" placeholder="City" />
        </div>
        <div class="mb-3">
            <label>Region</label>
            <textarea asp-for="@Model.Region" class="form-control" placeholder="Region"></textarea>
        </div>
        <div class="mb-3">
            <label>AddressDetail</label>
            <textarea asp-for="@Model.AddressDetail" class="form-control" placeholder="AddressDetail"></textarea>
        </div>
       

        <div class="mb-3">
            <button asp-area="Member" asp-controller="Order" asp-action="Add" class="btn btn-primary">Save</button>
            <button asp-controller="Cart" asp-action="List" class="btn btn-primary">Back To List</button>
        </div>
        <div asp-validation-summary="All" class="text-danger text-center">
        </div>
    </form>
</div>